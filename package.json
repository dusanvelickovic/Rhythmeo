{
    "name": "rhythmeo",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "start:server": "ts-node -P server/tsconfig.json server/src/main.ts",
        "start:server:watch": "nodemon --watch server --exec ts-node -P server/tsconfig.json server/src/main.ts",
        "start:all": "concurrently \"npm start\" \"npm run start:server:watch\"",
        "build:server": "tsc -p server/tsconfig.json"
    },
    "prettier": {
        "printWidth": 100,
        "singleQuote": true,
        "overrides": [
            {
                "files": "*.html",
                "options": {
                    "parser": "angular"
                }
            }
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^20.3.0",
        "@angular/compiler": "^20.3.0",
        "@angular/core": "^20.3.0",
        "@angular/forms": "^20.3.0",
        "@angular/platform-browser": "^20.3.0",
        "@angular/router": "^20.3.0",
        "@nestjs/common": "^11.1.6",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.1.6",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.1.6",
        "@nestjs/testing": "^11.1.6",
        "@nestjs/typeorm": "^11.0.0",
        "@tailwindcss/postcss": "^4.1.14",
        "cookie-parser": "^1.4.7",
        "passport": "^0.7.0",
        "passport-spotify": "^2.0.0",
        "pg": "^8.16.3",
        "postcss": "^8.5.6",
        "reflect-metadata": "^0.2.2",
        "rxjs": "~7.8.0",
        "tailwindcss": "^4.1.14",
        "tslib": "^2.3.0",
        "typeorm": "^0.3.27",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular/build": "^20.3.5",
        "@angular/cli": "^20.3.5",
        "@angular/compiler-cli": "^20.3.0",
        "@nestjs/cli": "^11.0.10",
        "@types/jasmine": "~5.1.0",
        "@types/passport-spotify": "^2.0.4",
        "concurrently": "^9.2.1",
        "jasmine-core": "~5.9.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "nodemon": "^3.1.10",
        "prettier": "3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "~5.9.2"
    }
}
